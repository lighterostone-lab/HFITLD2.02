{
    "shopItemsListElement": {
        "type": "list",
        "id": "list",
        "constants": {
            "onPurchaseSE": "Shop1"
        },
        "animations": [
            "field:opacity;duration:0.3;keyframes:0=25,100=255;delay:0.25",
            "field:y;relative;duration:0.2;keyframes:0=-20,100=0;delay:0.25"
        ],
        "parameters": {
            "maxCols": 2,
            "backgroundType": 2,
            "itemsPadding": 4
        },
        "bindings": {
            "size": { "width": "100%", "height": "86%" },
            "position": { "x": "center + 8hdp", "y": "center" }
        }
    }
}